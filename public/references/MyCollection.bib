issn = {00320862},
journal = {Plant Pathology},
keywords = {Chapter 3,Chapter 4,Host-plant resistance,Mi23 marker,REX-1 marker,Root-knot nematodes,Susceptibility,Tomato rootstock},
mendeley-tags = {Chapter 3,Chapter 4},
month = {dec},
number = {6},
pages = {1125--1135},
title = {{Variability in infection and reproduction of \textit{Meloidogyne javanica} on tomato rootstocks with the \textit{Mi} resistance gene}},
url = {http://doi.wiley.com/10.1111/j.1365-3059.2008.01906.x},
volume = {57},
year = {2008}
