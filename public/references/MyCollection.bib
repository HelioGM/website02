journal = {European Journal of Plant Pathology},
keywords = {Chapter 1,Chapter 2,Chapter 3,Cucumis sativus,Cucurbita maxima X Cucurbita moschata,Equilibrium density,Multiplication rate,Root-knot nematode,Tolerance limit},
mendeley-tags = {Chapter 1,Chapter 2,Chapter 3},
