@article{Davies2015,
abstract = {Plant-parasitic nematodes are a major pest of all agricultural systems, causing extensive economic losses. Natural resistance ( R ) genes offer an alternative to chemical control and have been shown effectively to limit nematode damage to crops in the field. Whilst a number of resistant cultivars have conferred resistance against root-knot and cyst nematodes for many decades, an increasing number of reports of resistance-breaking nematode pathotypes are beginning to emerge. The forces affecting the emergence of virulent nematodes are complex, multifactorial and involve both the host and parasite of the plant-nematode interaction. This review provides an overview of the root-knot and cyst nematode R genes characterised to date, in addition to examining the evolutionary forces influencing nematode populations and the emergence of virulence. Finally, potential strategies to improve R gene durability in the field are outlined, and areas that would benefit from further research efforts are highlighted.},
author = {Davies, Laura J. and Elling, Axel A.},
doi = {10.1163/15685411-00002877},
file = {:H$\backslash$:/Documentos/Mendeley Desktop/Davies, Elling - 2015 - Resistance genes against plant-parasitic nematodes a durable control strategy.pdf:pdf},
issn = {1388-5545},
journal = {Nematology},
keywords = {Chapter 4,R gene,control,cyst nematode,durability,root-knot nematode,selection,virulence},
mendeley-tags = {Chapter 4},
number = {3},
pages = {249--263},
title = {{Resistance genes against plant-parasitic nematodes: a durable control strategy?}},
url = {https://brill.com/view/journals/nemy/17/3/article-p249{\_}1.xml},
volume = {17},
year = {2015}
}
